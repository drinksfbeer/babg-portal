  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "concurrently": "^3.6.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-react": "^7.7.0",
    "redux-devtools-extension": "^2.13.5",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "scripts": {
    "start": "node server-build/index.js",
    "dev": "PORT=3002 node scripts/start.js",
    "dev-both": "./node_modules/concurrently/src/main.js \"npm run serve\" \"npm run dev\"",
    "serve2": "NODE_ENV=development babel server -w --out-dir server-build && nodemon server-build/index.js",
    "serve": "nodemon --watch server --watch build --watch public --exec 'babel server --out-dir server-build --quiet && node server-build/index.js'",
    "build:server": " NODE_ENV=production babel server -d server-build",
    "build": "node scripts/build.js && npm run build:server",
    "postinstall": "npm run build",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "react/jsx-wrap-multilines": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/no-array-index-key": 0,
      "react/no-unescaped-entities": 0,
      "import/no-named-as-default": 0,
      "import/no-named-as-default-member": 0,
      "import/no-extraneous-dependencies": 0,
      "import/newline-after-import": 0,
      "jsx-a11y/click-events-have-key-events": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "jsx-a11y/no-noninteractive-element-interactions": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "jsx-a11y/label-has-for": 0,
      "consistent-return": 0,
      "global-require": 0,
      "operator-linebreak": 0,
      "prefer-template": 0,
      "no-underscore-dangle": 0,
      "no-useless-escape": 0,
      "no-unused-expressions": 0,
      "no-cond-assign": 0,
      "no-plusplus": 0,
      "no-await-in-loop": 0,
      "no-prototype-builtins": 0,
      "no-param-reassign": 0,
      "no-shadow": 0
    },
    "env": {
      "browser": true,
      "node": true
    }
  }
}
